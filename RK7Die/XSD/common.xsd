<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">

    <xs:attributeGroup name="refItemAttributes">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Элемент справочника. Идентифицируется или id, или кодом, или guid.
                Обязательно заполнение одного из полей</xs:documentation>
        </xs:annotation>
        <xs:attribute name="id" type="xs:nonNegativeInteger"/>
        <xs:attribute name="code" type="xs:nonNegativeInteger"/>
        <xs:attribute name="guid" type="xs:normalizedString"/>
    </xs:attributeGroup>

    <xs:complexType name="refItem">
        <xs:attributeGroup ref="refItemAttributes"/>
        <xs:anyAttribute processContents="skip"/>
    </xs:complexType>

    <xs:complexType name="resRefItem">
        <xs:attribute name="id" type="xs:nonNegativeInteger"/>
        <xs:attribute name="code" type="xs:nonNegativeInteger"/>
        <xs:attribute name="name" type="xs:normalizedString"/>
    </xs:complexType>

    <xs:attributeGroup name="orderInfo">
        <xs:attribute name="orderIdent" type="xs:positiveInteger" use="required"/>
    </xs:attributeGroup>

    <xs:complexType name="orderElement">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Ссылка на заказ. Заказ можно выбрать либо по guid либо
                по visit+orderIdent. Если заданы все атрибуты, то сначала поиск выполняется по
                guid</xs:documentation>
        </xs:annotation>
        <xs:attribute name="guid" type="xs:normalizedString"/>
        <xs:attribute name="visit" type="xs:positiveInteger"/>
        <xs:attribute name="orderIdent" type="xs:positiveInteger"/>
    </xs:complexType>

    <xs:complexType name="checkItem">
        <xs:complexContent>
            <xs:extension base="refItem">
                <xs:attribute name="uni" type="xs:nonNegativeInteger">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">UNI элемента. Если задан, то элемент
                            обновляется, иначе создается новый. Только для нераспечатанных
                            элементов</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="line_guid" type="xs:normalizedString">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">GUID строки</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="state" type="CheckItemState">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">Статус элемента (с версии 7.5.4.206)</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:anyAttribute processContents="skip"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="modiItem">
        <xs:complexContent>
            <xs:extension base="checkItem">
                <xs:attribute name="price" type="xs:int" use="optional">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">Цена модификатора (в
                            копейках)</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="count" type="xs:positiveInteger" use="optional" default="1">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">Количество модификаторов</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="openName" type="xs:normalizedString" use="optional">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">Произвольное имя
                            модификатора</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="isDefault" type="xs:boolean" use="optional" default="1">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">Флаг - модификатор по
                            умолчанию</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="discountItem">
        <xs:complexContent>
            <xs:extension base="checkItem">
                <xs:all>
                    <xs:element name="Interface" type="refItem" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="ru">Интерфейс к карте гостя. Обезателен для
                                заполнения, если задан cardCode</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="BonusType" type="refItem" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="ru">Тип бонуса, заполняется для скидок ПДС</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:all>
                <xs:attribute name="amount" type="xs:int">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">Сумма скидки (в копейках), для скидок с
                            ручным вводом суммы скидки</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="maxamount" type="xs:int">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">Максимальная сумма скидки (в копейках)</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="seat" type="xs:nonNegativeInteger" use="optional" default="0">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">Номер посадочного места: 0 - не
                            задано</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="cardCode" type="xs:normalizedString">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">Код карточки</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="deleted" type="xs:boolean">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">Признак того что скидка удалена</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="voidItem">
        <xs:complexContent>
            <xs:extension base="checkItem">
                <xs:all>
                    <xs:element name="Author" type="refItem">
                        <xs:annotation>
                            <xs:documentation xml:lang="ru">Работник, выполняющий
                                удаление</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:all>
                <xs:attribute name="dateTime" type="xs:dateTime">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">ДатаВремя создания воида</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="quantity" type="xs:int" use="required">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">Количество удаляемого блюда (в тысячных
                            долях)</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="amount" type="xs:int">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">Сумма удаляемого блюда</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="openName" type="xs:normalizedString">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">Открытое имя причины
                            удаления</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="consumItem">
        <xs:complexContent>
            <xs:extension base="checkItem">
                <xs:all>
                    <xs:element name="Author" type="refItem">
                        <xs:annotation>
                            <xs:documentation xml:lang="ru">Работник, который добавил консуманта</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:all>
                <xs:attribute name="amount" type="xs:int">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">Сумма консумации (в копейках)</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="dishAncestor">
        <xs:complexContent>
            <xs:extension base="checkItem">
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element name="Modi" type="modiItem">
                        <xs:annotation>
                            <xs:documentation xml:lang="ru">Модификатор</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Discount" type="discountItem">
                        <xs:annotation>
                            <xs:documentation xml:lang="ru">Скидка</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Void" type="voidItem">
                        <xs:annotation>
                            <xs:documentation xml:lang="ru">Отказы (для распечатанных
                                блюд)</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Consum" type="consumItem">
                        <xs:annotation>
                            <xs:documentation xml:lang="ru">Консуманты</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:choice>
                <xs:attribute name="price" type="xs:nonNegativeInteger" use="optional">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">Цена блюда (в копейках)</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="dishItem">
        <xs:complexContent>
            <xs:extension base="dishAncestor">
                <xs:attribute name="quantity" type="xs:int" use="required">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">Количество блюда (в тысячных долях), для
                            добавляемых блюд. Если кол-во меньше нуля, то это блюдо выкупа, иначе обычная продажа</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="srcQuantity" type="xs:int" use="optional">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">Количество блюда, без учета отказов (в
                            тысячных долях, с 7.4.16.2)</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="seat" type="xs:nonNegativeInteger" use="optional" default="0">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">Номер посадочного места: 0 - не
                            задано</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="comboComponent">
        <xs:complexContent>
            <xs:extension base="dishAncestor">
                <xs:sequence>
                    <xs:element name="ComboModi" type="refItem" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="ru">Комбо-модификатор, соответствующий
                                комбо-компоненту. По умолчанию возьмется первый
                                подходящий</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="count" type="xs:positiveInteger" use="optional" default="1"/>
                <xs:attribute name="isDefault" type="xs:boolean" use="optional" default="1">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">Флаг - компонент по
                            умолчанию</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="comboItem">
        <xs:complexContent>
            <xs:extension base="dishItem">
                <xs:sequence>
                    <xs:element name="Modi" type="modiItem" minOccurs="0" maxOccurs="0"/>
                    <xs:element name="Component" type="comboComponent" minOccurs="0"
                        maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation xml:lang="ru">Комбо компонент</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
      
    <xs:simpleType name="TransactionStatusType">
        <xs:restriction base="xs:int">
            <xs:enumeration value="1">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Не выполнена</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="3">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">В процессе авторизации (для двухстадийной авторизации)</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="4">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Успешна</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="5">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Подтверждена</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="6">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Аннулирована</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:complexType name="payItem">
        <xs:complexContent>
            <xs:extension base="checkItem">
                <xs:sequence>
                    <xs:element name="Interface" type="refItem" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="ru">Интерфейс, связанный с
                                оплатой</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="amount" type="xs:int" use="required">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">Сумма (в копейках)</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="cardCode" type="xs:normalizedString" use="optional">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">Номер персональной карты</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="extTransactionInfo" type="xs:normalizedString" use="optional">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">Внешний код авторизации. Начиная с версии 7.5.3.111</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="TransactionStatus" type="TransactionStatusType" use="optional">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">Статус авторизации. Начиная с версии 7.5.4.211</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="seat" type="xs:nonNegativeInteger" use="optional" default="0">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">Номер посадочного места: 0 - не
                            задано</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="deleted" type="xs:boolean">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">Признак того что платеж удален</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="prepayItem">
        <xs:complexContent>
            <xs:extension base="payItem">
                <xs:sequence>
                    <xs:element name="Reason" type="refItem" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="ru">Причина внесения
                                предоплаты</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="promised" type="xs:boolean">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">Предоплата является обещанным платежом</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="PrintCheckItem">
        <xs:sequence>
            <xs:element name="Author" type="resEmployeeItem" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Кассир, создавший чек</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Reason" type="refItem" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Причина удаления чека</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DeleteManager" type="resEmployeeItem" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Работник, удаливший чек</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Pay" type="payItem" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Оплата</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Prepay" type="prepayItem" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Предоплата</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="uni" type="xs:nonNegativeInteger">
            <xs:annotation>
                <xs:documentation xml:lang="ru">UNI элемента. Если задан, то элемент обновляется,
                    иначе создается новый. Только для нераспечатанных элементов</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="line_guid" type="xs:normalizedString">
            <xs:annotation>
                <xs:documentation xml:lang="ru">GUID чека</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="seat" type="xs:nonNegativeInteger" use="optional" default="0">
            <xs:annotation>
                <xs:documentation xml:lang="ru">Номер посадочного места: 0 - не
                    задано</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="CheckNum" type="xs:nonNegativeInteger">
            <xs:annotation>
                <xs:documentation xml:lang="ru">Номер чека</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="invoice" type="xs:nonNegativeInteger">
            <xs:annotation>
                <xs:documentation xml:lang="ru">Номер накладной</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="amount" type="xs:positiveInteger" use="required">
            <xs:annotation>
                <xs:documentation xml:lang="ru">Сумма чека (в копейках)</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="deleted" type="xs:boolean">
            <xs:annotation>
                <xs:documentation xml:lang="ru">Флаг "Чек удален"</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="bill" type="xs:boolean">
            <xs:annotation>
                <xs:documentation xml:lang="ru">Флаг "Чек является пречеком"</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="printTime" type="xs:dateTime">
            <xs:annotation>
                <xs:documentation xml:lang="ru">Время печати чека</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="startTime" type="xs:dateTime">
            <xs:annotation>
                <xs:documentation xml:lang="ru">Время начала обслуживания чека</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="sessionAncestor">
        <xs:attribute name="uni" type="xs:int">
            <xs:annotation>
                <xs:documentation xml:lang="ru">UNI пакета. Если задан, то перезаписывается
                    содержимое пакета. Иначе создается новый пакет</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="line_guid" type="xs:normalizedString">
            <xs:annotation>
                <xs:documentation xml:lang="ru">GUID пакета</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="isDraft" type="xs:boolean">
            <xs:annotation>
                <xs:documentation xml:lang="ru">Флаг "Пакет является черновиком"</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="printed" type="xs:boolean" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="ru">Флаг - пакет распечатан</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="printTime" type="xs:dateTime">
            <xs:annotation>
                <xs:documentation xml:lang="ru">Время, в которое блюда были добавлены в заказ. Если
                    не задано, то используется время remindAt</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="remindTime" type="xs:dateTime">
            <xs:annotation>
                <xs:documentation xml:lang="ru">Время начала готовки блюд пакета. Если не задано, то
                    используется текущее время</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="readyTime" type="xs:dateTime">
            <xs:annotation>
                <xs:documentation xml:lang="ru">Время, к которому должны быть приготовлены все блюда
                    пакета</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="startService" type="xs:dateTime">
            <xs:annotation>
                <xs:documentation xml:lang="ru">Время начала обслуживания</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:anyAttribute processContents="skip"/>
    </xs:complexType>

    <xs:complexType name="SessionItem">
        <xs:complexContent>
            <xs:extension base="sessionAncestor">
                <xs:sequence>
                    <xs:element name="Station" type="refItem" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="ru">Станция, на которой пакет был
                                добавлен</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Author" type="refItem" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="ru">Работник, последний редактировавший
                                заказ</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Creator" type="refItem" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="ru">Работник, создавший
                                пакет</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Course" type="refItem" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="ru">Порядок подачи</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:choice minOccurs="0" maxOccurs="unbounded">
                        <xs:element name="Dish" type="dishItem">
                            <xs:annotation>
                                <xs:documentation xml:lang="ru">Блюдо</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="Combo" type="comboItem">
                            <xs:annotation>
                                <xs:documentation xml:lang="ru">Комбо блюдо</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="Discount" type="discountItem">
                            <xs:annotation>
                                <xs:documentation xml:lang="ru">Скидка</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="Prepay" type="prepayItem">
                            <xs:annotation>
                                <xs:documentation xml:lang="ru">Предоплата</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:choice>
                </xs:sequence>
                <xs:attribute name="fixedPrice" type="xs:boolean">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">Флаг "Зафиксировать цены"</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:simpleType name="KDSStateType">
        <xs:annotation>
            <xs:documentation xml:lang="ru">КДС статус блюда (с версии 7.5.5.036)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:normalizedString">
            <xs:enumeration value=""/>
            <xs:enumeration value="sent">
                <xs:annotation>
